{
  "name": "propertypro",
  "version": "1.0.0",
  "description": "PropertyPro is a platform where people can create and/or search property for sale or rent",
  "main": "server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test npm run db:migrate && cross-env NODE_ENV=test nyc mocha --require @babel/register --no-timeout './Server/tests/*.js' --exit",
    "coverage": "npm test && nyc report --reporter=lcov --reporter=text-lcov | coveralls",
    "build": "babel Server --out-dir build",
    "start": "set DEBUG=*:server && node build/app.js",
    "dev:start": "set DEBUG=*:server && nodemon --exec babel-node ./Server/app.js",
    "migrate-db": "set DEBUG=*:database && node ./build/migration/table.js",
    "db:migrate": "set DEBUG=*:database && babel-node ./Server/migration/table.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Abobos/Property-Pro-Lite.git"
  },
  "author": "Blessing Makaraba",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Abobos/Property-Pro-Lite/issues"
  },
  "homepage": "https://github.com/Abobos/Property-Pro-Lite#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.14.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "mocha": "^6.1.4",
    "multer": "^1.4.1",
    "nodemailer": "^6.2.1",
    "nyc": "^14.1.1",
    "pg": "^7.11.0",
    "swagger-ui-express": "^4.0.6"
  }
}
